@model AddMenuTable
@{
    ViewData["Title"] = "AddMenuTable";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}


<form class="form-valide" method="post">
	<div class="col-md-12">
		<div class="card">
			<div class="card-header">
				<div class="card-title">Masa Ekleme Sayfası</div>
			</div>

			<div class="card-body">
				
				<div class="form-group">
					<label for="text">Masa Adı</label>
					<input type="text" class="form-control" asp-for="Name" placeholder="Masa Adı">
				</div>
			</div>
			<div class="card-action">
				<button class="btn btn-success" type="submit">Ekle</button>
				<a class="btn btn-danger" href="/MenuTable/MenuTableList">Geri</a>
			</div>
		</div>
	</div>
</form>